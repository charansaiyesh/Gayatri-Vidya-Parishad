<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/departmentFragment"
        android:name="com.example.gayatrividhyaparishad.DepartmentFragment"
        android:label="fragment_department"
        tools:layout="@layout/fragment_department" >
        <action
            android:id="@+id/action_departmentFragment_to_cseFragment"
            app:destination="@id/cseFragment" />
        <action
            android:id="@+id/action_departmentFragment_to_eceFragment"
            app:destination="@id/eceFragment" />
        <action
            android:id="@+id/action_departmentFragment_to_eeeFragment"
            app:destination="@id/eeeFragment" />
        <action
            android:id="@+id/action_departmentFragment_to_mechFragment"
            app:destination="@id/mechFragment" />
        <action
            android:id="@+id/action_departmentFragment_to_chemFragment"
            app:destination="@id/chemFragment" />
        <action
            android:id="@+id/action_departmentFragment_to_civilFragment"
            app:destination="@id/civilFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.gayatrividhyaparishad.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_departmentFragment"
            app:destination="@id/departmentFragment" />
        <action
            android:id="@+id/action_homeFragment_to_adminFragment"
            app:destination="@id/adminFragment" />
    </fragment>
    <fragment
        android:id="@+id/cseFragment"
        android:name="com.example.gayatrividhyaparishad.CseFragment"
        android:label="fragment_cse"
        tools:layout="@layout/fragment_cse" />
    <fragment
        android:id="@+id/adminFragment"
        android:name="com.example.gayatrividhyaparishad.AdminFragment"
        android:label="fragment_admin"
        tools:layout="@layout/fragment_admin" />
    <fragment
        android:id="@+id/eceFragment"
        android:name="com.example.gayatrividhyaparishad.EceFragment"
        android:label="fragment_ece"
        tools:layout="@layout/fragment_ece" />
    <fragment
        android:id="@+id/eeeFragment"
        android:name="com.example.gayatrividhyaparishad.EeeFragment"
        android:label="fragment_eee"
        tools:layout="@layout/fragment_eee" />
    <fragment
        android:id="@+id/mechFragment"
        android:name="com.example.gayatrividhyaparishad.MechFragment"
        android:label="fragment_mech"
        tools:layout="@layout/fragment_mech" />
    <fragment
        android:id="@+id/chemFragment"
        android:name="com.example.gayatrividhyaparishad.ChemFragment"
        android:label="fragment_chem"
        tools:layout="@layout/fragment_chem" />
    <fragment
        android:id="@+id/civilFragment"
        android:name="com.example.gayatrividhyaparishad.CivilFragment"
        android:label="fragment_civil"
        tools:layout="@layout/fragment_civil" />
</navigation>